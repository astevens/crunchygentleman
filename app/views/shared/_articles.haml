            / Inputs
            / Use class .small, .medium or .large for predefined size
            %fieldset
              %legend Legend
              %dl
                %dt
                  %label Short Input
                %dd
                  %input.small{:type => "text"}/
                  %p Short description of event
                %dt
                  %label Medium Input
                %dd
                  %input.medium{:type => "text"}/
                %dt
                  %label Focused Input
                %dd
                  %input.medium{:type => "text"}/
                %dt
                  %label Valid Input
                %dd
                  %input.medium.valid{:type => "text"}/
                  %span.valid-side-note Valid text
                %dt
                  %label Invalid Input
                %dd
                  %input.medium.invalid{:type => "text"}/
                  %span.invalid-side-note Invalid text
                %dt
                  %label Disabled Input
                %dd
                  %input.medium{:disabled => "", :type => "text"}/
                %dt
                  %label Long text input
                %dd
                  %input.large{:type => "text"}/
                  %p Short description of event
                %dt
                  %label File input
                %dd
                  %input.fileupload{:type => "file"}/
                  %p Max size 5Mb
                %dt
                  %label Date input
                %dd
                  %input.datepicker.small{:type => "text"}/
                %dt
                  %label Textarea
                %dd
                  / Use class .wysiwyg for jQuery jWYSIWYG initiation
                  %textarea.wysiwyg.large{:cols => "40", :rows => "5"}
                  %p Short description of event
            %fieldset
              %legend Check-in some of these
              %dl
                %dt.checkbox
                  %label Checkbox A
                %dd
                  %input{:type => "checkbox"}/
                %dt.checkbox
                  %label Checkbox B
                %dd
                  %input{:type => "checkbox"}/
                %dt.checkbox
                  %label Checkbox C
                %dd
                  %input{:type => "checkbox"}/
            %fieldset
              %legend Choose one of these
              %dl
                %dt.radio
                  %label Option 1
                %dd
                  %input{:name => "test", :type => "radio"}/
                %dt.radio
                  %label Option 2
                %dd
                  %input{:name => "test", :type => "radio"}/
                %dt.radio
                  %label Option 3
                %dd
                  %input{:name => "test", :type => "radio"}/
            %fieldset
              %legend Choose one of these
              %dl
                %dt &nbsp;
                %dd
                  %select{:name => "actions"}
                    %option{:selected => ""} Choose an action
                    %option{:value => "e"} Edit
                    %option{:value => "r"} Remove
                    %option{:value => "x"} Export
            %button{:type => "submit"} Submit
            or
            %a{:href => "#"} Cancel
